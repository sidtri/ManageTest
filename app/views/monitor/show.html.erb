
<div class='highlight'><center><b><%= Project.find(params[:id]).name%></b></center></div>

<span class="badge pull-right">0</span>
<%=link_to 'Inbox',tester_index_path,:class=>'btn btn-default pull-right' %>


<ol class="breadcrumb pull-left">
  <li><a href="<%=root_path %>">Projects</a></li>
  <li><a href="<%=initiate_path %>">Testers</a></li>
  <li class="tester_index_path">Data</li>
</ol>

<table class='table'>
<caption class="text-left" style="color:#679BB7;">Test Progress</caption>
<tbody><tr>
<th>In Date</th>
<th>Project </th>
<th>Tester</th>
<th>Environment</th>
<th>Out Date</th>
</tr>
</tbody>
	<%@initiation.each do |i| %>
	<tr>
	<td class='code'><%= i.created_at.strftime('%d - %m - %Y') %></td>
	<td><%= i.project.name %></td>
	<td><%= i.name %></td>
	<td><%= i.environment %></td>
	<td class='code'><%= i.updated_at.strftime('%d - %m - %Y') %></td>
	</tr>
	<%end %>
</table>

